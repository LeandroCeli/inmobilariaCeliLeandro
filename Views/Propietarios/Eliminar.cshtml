@model inmobilariaCeli.Models.Propietario

<h2>Eliminar Propietario</h2>
<h4>¿Estás seguro de eliminar este registro?</h4>

<dl class="row">
    <dt class="col-sm-2">DNI</dt>
    <dd class="col-sm-10">@Model.DNI</dd>
    <dt class="col-sm-2">Nombre</dt>
    <dd class="col-sm-10">@Model.Apellido @Model.Nombre</dd>
</dl>

<form asp-action="DeleteConfirmed">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
